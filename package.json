  "dependencies": {
    "typescript": "^2.2.1"
  },
  "devDependencies": {
    "@types/chai": "^3.4.35",
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.8",
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "rollup": "^0.41.5",
    "rollup-plugin-typescript": "^0.8.1",
    "tslint": "^4.4.2"
  }
